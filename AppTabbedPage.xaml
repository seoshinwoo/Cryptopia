<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:TabbedPageSample;assembly=TabbedPage"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            x:Class="TabbedPageSample.AppTabbedPage">

    <NavigationPage Title="One" IconImageSource="{StaticResource TabOne}">
        <x:Arguments>
            <local:MainPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Two" IconImageSource="{StaticResource TabTwo}">
        <x:Arguments>
            <local:VideoPage Title="Two" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Three" IconImageSource="{StaticResource TabThree}">
        <x:Arguments>
            <local:LinkPage Title="Three" />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
